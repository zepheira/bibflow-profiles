{
    "Profile": {
        "title": "Music",
        "resourceTemplate": [
            {
                "id": "bfp:Work:Music",
                "class": {
                    "classLabel": "Music",
                    "id": "http://bibfra.me/vocab/marc/Music",
                    "propertyTemplate": [
                        {
                            "mandatory": "true",
                            "property": {
                                "id": "http://bibfra.me/vocab/lite/title",
                                "propertyLabel": "Album name"
                            },
                            "repeatable": "false",
                            "type": "literal"
                        },
                        {
                            "property": {
                                "id": "http://bibfra.me/vocab/relation/creator",
                                "propertyLabel": "Creator"
                            },
                            "repeatable": "true",
                            "type": "resource",
                            "valueConstraint": {
                                "descriptionTemplateRef": [ "bfp:Agent:Person", "bfp:Agent:Organization", "bfp:Agent:Family" ]
                            }
                        },
                        {
                            "property": {
                                "id": "http://bibfra.me/vocab/lite/description",
                                "propertyLabel": "Description"
                            },
                            "type": "literal"
                        },
                        {
                            "property": {
                                "id": "http://bibfra.me/vocab/lite/subject",
                                "propertyLabel": "Subject"
                            },
                            "repeatable": "true",
                            "type": "resource",
                            "valueConstraint": {
                                "descriptionTemplateRef": [ "bfp:Agent:Person", 
							    "bfp:Agent:Organization", 
							    "bfp:Authority:Topic" ]
                            }
                        },
                        {
                            "property": {
                                "id": "http://bibfra.me/vocab/lite/language",
                                "propertyLabel": "Language"
                            },
                            "repeatable": "true",
                            "type": "resource",
                            "valueConstraint": {
                                "descriptionTemplateRef": "bp:language"
                            }
                        },
                        {
                            "property": {
                                "id": "http://bibfra.me/vocab/relation/recordingengineer",
                                "propertyLabel": "Recording Engineer"
                            },
                            "repeatable": "true",
                            "type": "resource",
                            "valueConstraint": {
                                "descriptionTemplateRef": [ "bfp:Agent:Person", "bfp:Agent:Organization" ]
                            }
                        },
                        {
                            "property": {
                                "id": "http://bibframe.org/vocab/av/track",
                                "propertyLabel": "Track"
                            },
                            "repeatable": "true",
                            "type": "resource",
                            "valueConstraint": {
                                "descriptionTemplateRef": [ "bfp:Audio:Track" ]
                            }
                        }
                    ]
                }
            },
            {
                "id": "vinyl",
                "class": {
                    "classLabel": "Vinyl Record",
                    "id": "http://bibframe.org/vocab/proposed/Vinyl",
                    "instantiates": "http://bibfra.me/vocab/marc/Music",
                    "propertyTemplate": [
                        {
                            "property": {
                                "id": "http://bibframe.org/vocab/provider",
                                "propertyLabel": "Label"
                            },
                            "repeatable": "true",
                            "type": "resource",
                            "valueConstraint": {
                                "descriptionTemplateRef": [ "bfp:Event:DistributeEvent" ]
                            }
                        },
			{
                            "property": {
                                "id": "http://bibfra.me/vocab/lite/image",
                                "propertyLabel": "Album Cover"
                            },
                            "type": "resource",
                            "valueConstraint": {
                                "descriptionTemplateRef": "e:Image"
                            }
			}
                    ]
                }
            },
            {
                "id": "bfp:Audio:Track",
                "class": {
                    "classLabel": "Track",
                    "id": "http://bibframe.org/vocab/av/Track",
                    "propertyTemplate": [
                        {
                            "property": {
                                "id": "http://bibfra.me/vocab/lite/label",
                                "propertyLabel": "Name"
                            },
                            "type": "literal"
                        },
                        {
                            "property": {
                                "id": "http://bibfra.me/vocab/relation/creator",
                                "propertyLabel": "Creator"
                            },
                            "repeatable": "true",
                            "type": "resource",
                            "valueConstraint": {
                                "descriptionTemplateRef": [ "bfp:Agent:Person", "bfp:Agent:Organization", "bfp:Agent:Family" ]
                            }
                        },
                        {
                            "property": {
                                "id": "http://bibframe.org/vocab/duration",
                                "propertyLabel": "Duration"
                            },
                            "type": "literal"
                        }
                    ]
                }
            },
            {
                "id": "e:Image",
                "class": {
                    "classLabel": "Image",
                    "id": "http://example.org/vocab/bfp/Image",
                    "propertyTemplate": [
                        {
                            "property": {
                                "id": "http://example.org/vocab/bfp/filename",
                                "propertyLabel": "File Name"
                            },
                            "type": "literal"
                        },
                        {
                            "property": {
                                "id": "http://example.org/vocab/bfp/mimetype",
                                "propertyLabel": "MIME-Type"
                            },
                            "type": "literal"
                        },
                        {
                            "property": {
                                "id": "http://bibframe.org/vocab/width",
                                "propertyLabel": "Width"
                            },
                            "type": "literal"
                        },
                        {
                            "property": {
                                "id": "http://bibframe.org/vocab/height",
                                "propertyLabel": "Height"
                            },
                            "type": "literal"
                        }
                    ]
                }
            }
        ]
    }
}
